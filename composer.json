{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=8.0.2",
        "bosnadev/database": "dev-master as 0.21.1",
        "spatie/laravel-ignition": "^1.0",
        "guzzlehttp/guzzle": "^7.4",
        "laravel-lang/lang": "^10.7.1",
        "laravel/framework": "^9.0",
        "laravel/tinker": "~2.0",
        "laravel/ui": "^3.0",
        "lsolesen/pel": "^0.9.12",
        "mstaack/laravel-postgis": "^5.3",
        "nicolaslopezj/searchable": "^1.13.0",
        "phpoffice/phppresentation": "^1.0.0",
        "phpoffice/phpspreadsheet": "^1.22.0",
        "phpoffice/phpword": "^0.18.3",
        "renanbr/bibtex-parser": "^2.1.2",
        "spatie/laravel-activitylog": "^4.5.2",
        "spatie/laravel-permission": "^5.5.2",
        "tecnickcom/tcpdf": "^6.4.4",
        "php-open-source-saver/jwt-auth": "^1.4.2",
        "wapmorgan/unified-archive": "^1.1.4"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "dms/phpunit-arraysubset-asserts": "^0.2.1",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "~9.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/bosnadev/database.git"
        }
    ],
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
