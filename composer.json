{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=8.2.0",
        "clickbar/laravel-magellan": "^2.0.0",
        "fileeye/pel": "^0.12.0",
        "guzzlehttp/guzzle": "^7.4",
        "laravel-lang/lang": "^15.19.7",
        "laravel/fortify": "^1.25",
        "laravel/framework": "^12.0",
        "laravel/reverb": "^1.4.0",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "~2.0",
        "laravel/ui": "^4.5",
        "nicolaslopezj/searchable": "^1.13.0",
        "phpoffice/phppresentation": "^1.1.0",
        "phpoffice/phpspreadsheet": "^1.9.0",
        "phpoffice/phpword": "^1.3.0",
        "renanbr/bibtex-parser": "^2.2.0",
        "spatie/laravel-activitylog": "^4.10.0",
        "spatie/laravel-ignition": "^2.9.0",
        "spatie/laravel-permission": "^6.16.0",
        "spatie/laravel-searchable": "^1.13",
        "tecnickcom/tcpdf": "^6.8.2",
        "wapmorgan/unified-archive": "^1.1.10"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^2.0",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
